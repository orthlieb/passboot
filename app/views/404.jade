doctype 5
html(lang="en")
	head
		meta(charset="utf-8")
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		meta(name="description", content="")
		meta(name="author", content="")
		link(rel="shortcut icon", href="/ico/favicon.png")
		title Passboot Error
		
		// Bootstrap core CSS
		link(href="/bootstrap/css/bootstrap.css", rel="stylesheet")
		
		// Custom styles for this template
		link(href="/css/404.css", rel="stylesheet")

		script(src="/jquery/jquery.js")
		script(src="/jquery/jquery.validate.js")
		script(src="/bootstrap/js/bootstrap.min.js")
	body
		include jumbotron
		+jumbotron("El Bandito Found An Error", "/img/Bandito.png")
		.container
			.row
				.col-md-12
					.account-wall
						.error-template
							h1 Oops!
							h2 404 Not Found
							.error-details
								| Sorry, an error has occurred, requested page #{url} not found!
							.error-actions
								a(href="/").btn.btn-primary.btn-lg
									span.glyphicon.glyphicon-home
									|  Take Me Back to the Ranch 
								a(href="/").btn.btn-default.btn-lg
									span.glyphicon.glyphicon-envelope
									|  Contact the Sheriff
